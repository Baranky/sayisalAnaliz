import math
import numpy
def f(x):
  return x**3-2*x**2-5
ustdeger=4
altdeger=2
deger1=f(altdeger)
deger2=f(ustdeger)
if deger1 * deger2 < 0:
 for i in range(1,5):
     c=(altdeger+ustdeger)/2
     deger3=f(c)
     if (deger1+ deger3<0):
         ustdeger=c
         deger2=deger3
     else:
        altdeger=c
        deger1=deger3
print("c=",c)
